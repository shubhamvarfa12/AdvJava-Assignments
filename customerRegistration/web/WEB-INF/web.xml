<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginD</servlet-name>
        <servlet-class>LoginD.LoginD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginDemo</servlet-name>
        <servlet-class>LoginDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationD</servlet-name>
        <servlet-class>RegistrationD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MenuDemo</servlet-name>
        <servlet-class>MenuDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchDemo</servlet-name>
        <servlet-class>SearchDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>displayAll</servlet-name>
        <servlet-class>displayAll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDemo</servlet-name>
        <servlet-class>DeleteDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateDemo</servlet-name>
        <servlet-class>updateDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateD</servlet-name>
        <servlet-class>updateD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutDemo</servlet-name>
        <servlet-class>LogoutDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginD</servlet-name>
        <url-pattern>/LoginD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginDemo</servlet-name>
        <url-pattern>/LoginDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationD</servlet-name>
        <url-pattern>/RegistrationD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MenuDemo</servlet-name>
        <url-pattern>/MenuDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchDemo</servlet-name>
        <url-pattern>/SearchDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>displayAll</servlet-name>
        <url-pattern>/displayAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDemo</servlet-name>
        <url-pattern>/DeleteDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateDemo</servlet-name>
        <url-pattern>/updateDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateD</servlet-name>
        <url-pattern>/updateD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutDemo</servlet-name>
        <url-pattern>/LogoutDemo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
