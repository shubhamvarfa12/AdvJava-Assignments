<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Regis</servlet-name>
        <servlet-class>Regis.Regis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>delete.delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete1</servlet-name>
        <servlet-class>delete1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>myFIlter</servlet-name>
        <servlet-class>myFIlter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Regis</servlet-name>
        <url-pattern>/Regis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete1</servlet-name>
        <url-pattern>/delete1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>myFIlter</servlet-name>
        <url-pattern>/myFIlter</url-pattern>
    </servlet-mapping>
    <session-config>
       
         <session-timeout>
            30
        </session-timeout>
    </session-config>
         
        <!--<Filter>-->
        <filter>
            <filter-name>filter1</filter-name>
            <filter-class>myFIlter</filter-class>
        </filter>
        
        <filter-mapping>
            <filter-name>filter1</filter-name>
            <url-pattern>/Login</url-pattern>
        </filter-mapping>  
        
        
       
</web-app>
